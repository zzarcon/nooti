{
  "name": "nooti",
  "version": "0.1.0",
  "description": "",
  "main": "./dist/nooti.js",
  "scripts": {
    "start": "npm run serve | npm run dev",
    "serve": "./node_modules/.bin/http-server -p 8080",
    "dev": "webpack-dev-server --progress --colors --port 8090",
    "watch": "webpack --progress --colors --watch",
    "build": "node_modules/webpack/bin/webpack.js",
    "amend": "git add . && git commit --amend --reuse-message=HEAD",
    "push": "git push --tags && git push",
    "release": "bower version patch -f && npm version patch --force --no-git-tag-version && npm run build && npm run amend && npm run push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zzarcon/nooti.git"
  },
  "keywords": [
    "nooti",
    "react-component",
    "title",
    "notification",
    "noty",
    "document",
    "document title"
  ],
  "author": "zzarcon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zzarcon/nooti/issues"
  },
  "homepage": "https://github.com/zzarcon/nooti",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^5.4.3",
    "babel-loader": "^5.1.2",
    "http-server": "~0.7.1",
    "webpack": "~1.4.4",
    "webpack-dev-server": "~1.6.5"
  }
}